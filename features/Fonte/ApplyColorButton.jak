public refines class ApplyColorButton {
public void foo() {
		
		if(status.getButtonID().equals("Fonte")) {
			String tagStyle = "color: #";
			String colorName = new ConverterColorToString().converterColor(status.getColor());
			WebHelpBar.applyButtonStatus.removeFontStyle(tagStyle);
			WebHelpBar.applyButtonStatus.setFontStyle(tagStyle + colorName + ";", 
					status.isActive());
		}
		Super().foo() ;
	}
}